package com.practicajava;


public class Practicajava {

    public static void main(String[] args) {
        int resultado;
        resultado = suma(1,5,6);
        
        System.out.println("El resultado es: "+resultado);
        
        
    }
    
    public static int suma(int a, int b, int c){
        
        return a+b+c;
    }
}
